{
    "version": 2,
    "builds": [
        {
            "src": "api/get-prices.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/get-prices",
            "dest": "/api/get-prices.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}